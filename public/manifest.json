{
  "manifest_version": 2,
  "name": "Sound mixer",
  "version": "1.1.0",
  "description": "Chrome extension to control any tab's volume",
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-694kn7MaUd8urE9lMnVq2Zul5NHwHBnx62mFSakTkgA='",
  "permissions": [
    "activeTab",
    "tabCapture",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "./logo.png",
    "48": "./logo.png",
    "128": "./logo.png",
    "256": "./logo.png",
    "512": "./logo.png"
  }
}